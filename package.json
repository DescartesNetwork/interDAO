  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.23",
    "chai": "^4.3.4",
    "gh-pages": "^3.2.3",
    "mocha": "^9.2.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.3.5"
  }
