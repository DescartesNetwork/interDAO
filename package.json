{
  "name": "@interdao/core",
  "version": "0.1.0",
  "main": "dist/lib/index.js",
  "scripts": {
    "start": "nodemon lib/index.ts",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "predeploy": "typedoc --excludePrivate --excludeProtected ./lib/index.ts",
    "deploy": "echo js.interdao.so >> ./docs/CNAME && gh-pages -d docs",
    "test": "npm run build && mocha --require ts-node/register -t 120000 lib-test/**/*.test.ts"
  },
  "author": "Tu Phan <tuphan@descartes.network>",
  "license": "MIT",
  "dependencies": {
    "@project-serum/anchor": "^0.23.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "ts-mocha": "^8.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "typescript": "^4.3.5"
  }
}
